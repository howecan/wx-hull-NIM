/* pages/movie-more/movie-more.wxss */
@import "/pages/movie-list/movie-list.wxss";
.session-tab{
  width:100%;
  display:none;
  position:fixed;
  background: #fff;
  z-index:1;
  border-width:1rpx 0;
  border-style:solid;
  border-color:#e9e9e9;
}
.tab-name{
  flex:1;
  font-size:32rpx;
  font-weight: 600;
  height:84rpx;
  line-height: 84rpx;
  text-align: center;

}
.tab-select{
  color:#32cd32;
  border-bottom:2px solid #32cd32;
}
.normal{
  color:#9e9e9e;
}
.session-content{
  margin-top:0rpx;
}